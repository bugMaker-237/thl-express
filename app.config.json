{
  "license": "All rights reserved",
  "version": "0.0.1",
  "authorization": {
    "authorisedDomains": ["/(localhost:8000).*/"],
    "authorizationHeaderName": "Authorization",
    "authorizationScheme": "Bearer ",
    "throwNoTokenError": false,
    "skipWhenExpired": true,
    "blackListedRoutes": ["/sign-in", "/check-username"]
  },
  "apiEndpoints": {
    "user": {
      "serviceName": "user",
      "servicePath": "localhost:9001/users/",
      "version": "0.0.1"
    }
  },
  "cacheConfig": {
    "unCachableRoutes": ["/sign-in", "/check-username"]
  }
}
