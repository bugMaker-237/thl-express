  "license": "All rights reserved",
  "version": "0.0.1",
  "contacts": {
    "support": "contact@thl-express.com",
    "issue": "contact@thl-express.com",
    "faq": "contact@thl-express.com"
  },
  "authorization": {
    "appDomains": ["thl-express.com"],
    "authorizationHeaderName": "Authorization",
    "authorizationScheme": "Bearer ",
    "throwNoTokenError": true,
    "continueIfTokenExpired": false,
    "anonymousRoutes": [
      "/api/login",
      "/api/register",
      "/api/validate",
      "/api/resend",
      "/api/settings",
      "/api/client/languages/en",
      "/api/client/languages/fr",
      "/api/driver/languages/en",
      "/api/driver/languages/fr"
    ]
  },
  "apiEndpoints": {
    "only": {
      "serviceHost": "https://thl-express.com",
      "serviceName": "only",
      "servicePath": "https://thl-express.com/api",
      "version": "0.0.1"
    },
    "client": {
      "serviceHost": "https://thl-express.com",
      "serviceName": "client",
      "servicePath": "https://thl-express.com/api/client",
      "version": "0.0.1"
    },
    "driver": {
      "serviceHost": "https://thl-express.com",
      "serviceName": "driver",
      "servicePath": "https://thl-express.com/api/driver",
      "version": "0.0.1"
    }
  },
  "cacheConfig": {
    "unCachableRoutes": [
      "/api/login",
      "/api/register",
      "/api/validate",
      "/api/resend",
      "/api/logout",
      "/api/settings"
    ]
  }
}
