  },
  "authorization": {
    "appDomains": ["thl.sellamgo.com"],
    "authorizationHeaderName": "Authorization",
    "authorizationScheme": "Bearer ",
    "throwNoTokenError": true,
    "continueIfTokenExpired": false,
    "anonymousRoutes": [
      "/api/login",
      "/api/logout",
      "/api/register",
      "/api/validate",
      "/api/resend",
      "/api/settings"
    ]
  },
  "apiEndpoints": {
    "only": {
      "serviceHost": "http://thl.sellamgo.com",
      "serviceName": "only",
      "servicePath": "http://thl.sellamgo.com/api",
      "version": "0.0.1"
    },
    "client": {
      "serviceHost": "http://thl.sellamgo.com",
      "serviceName": "client",
      "servicePath": "http://thl.sellamgo.com/api/client",
      "version": "0.0.1"
    }
  },
  "cacheConfig": {
    "unCachableRoutes": [
      "/api/login",
      "/api/register",
      "/api/validate",
      "/api/resend",
      "/api/logout",
      "/api/settings"
    ]
  }
}
